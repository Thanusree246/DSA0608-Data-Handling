# Data
product_names <- c('Product A', 'Product B', 'Product C', 'Product D', 'Product E')
quantities <- c(250, 175, 300, 200, 220)

# Line plot
plot(1:length(product_names), quantities, type = "o", col = "blue", xaxt = "n", xlab = "Product ID", ylab = "Quantity Available", main = "Product Inventory - Line Plot")
axis(1, at = 1:length(product_names), labels = product_names)
